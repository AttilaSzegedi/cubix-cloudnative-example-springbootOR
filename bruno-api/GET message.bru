meta {
  name: GET message
  type: http
  seq: 2
}

get {
  url: http://localhost:8080/demo/message?message=Hello From Postman!
  body: none
  auth: none
}

query {
  message: Hello From Postman!
}

assert {
  res.status: eq 200
}

tests {
  test("Query param should appear in the response", function() {
    const message = req.getUrl().split("?")[1].split("=")[1];
    expect(res.getBody().message).to.equal(message);
  });
  
}
